{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "noEmit": false,
    "outDir": ".test-dist",
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "ES2020",
    "jsx": "react-jsx",
    "allowImportingTsExtensions": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": [
    "src/lib/api-types.ts",
    "src/lib/api.ts",
    "src/lib/auth.ts",
    "src/lib/navigation.ts",
    "src/lib/auth.test.ts"
  ]
}
